



pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
             stage('build') {
            steps {
                echo 'Hello build'
                sleep 5
            }
        }
             stage('deploy') {
            steps {
                echo 'Hello deploy'
                sleep 5
            }
        }
           stage('test') {
            steps {
                echo 'Hello test'
                sleep 4
            }
        }  
    }
}

